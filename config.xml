<widget xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0" id="com.phonegap.helloworld" version="1.0.0">

<name>CNHS</name>

<description> An app to alert students of notifications at Cardinal Newman High School in Santa Rosa, CA. </description>

<author email="ethandburrell@gmail.com" href="http://cardinalnewman.org"> Ethan Burrell + Steven Bucher </author>
    
<content src="calendar.html"/>

<plugin name="cordova-plugin-whitelist" source="npm" />

<plugin name="phonegap-plugin-push" spec="1.6.0">
    <param name="SENDER_ID" value="244192342021" />
</plugin>
<plugin name="cordova-plugin-device" source="npm" />

<access origin="*" />
<access origin="https://secure-island-66271.herokuapp.com/calendar/" />
<platform name="android">
    <preference name="orientation" value="portrait" />
    <icon src="icon.png" />
<!-- a preference specific to android only here -->
</platform>
<platform name="ios" >
    <preference name="orientation" value="portrait" />
<icon src="res/icon/ios/Icon-Small-50.png"            platform="ios"        width="57" height="57" />
    <icon src="res/icon/ios/Icon-72.png"            platform="ios"        width="72" height="72" />
    <icon src="res/icon/ios/Icon-Small-50@2x.png"         platform="ios"        width="114" height="114" />
    <icon src="res/icon/ios/Icon-72@2x.png"         platform="ios"        width="144" height="144" />
    <icon src="res/icon/ios/Icon-60@2x.png"         platform="ios"        width="120" height="120" />
    <icon src="res/icon/ios/Icon-76@2x.png"         platform="ios"        width="152" height="152" />
    <icon src="res/icon/ios/Icon-76.png"         platform="ios"        width="76" height="76" />
    <icon src="res/icon/ios/Icon-60@3x.png"         platform="ios"        width="180" height="180" />
</platform>
<feature name="Device">
<param name="android-package"
value="org.apache.cordova.device.Device"/>

</feature>
</widget>
